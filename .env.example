# Configuration OneFlex
# Pour SSO : utilisez ONEFLEX_TOKEN et ONEFLEX_REFRESH_TOKEN (recommandé)
ONEFLEX_TOKEN=
ONEFLEX_REFRESH_TOKEN=

# Pour authentification classique (ne fonctionne pas avec SSO)
ONEFLEX_EMAIL=votre.email@example.com
ONEFLEX_PASSWORD=votre_mot_de_passe

# IDs optionnels pour forcer un bureau/espace spécifique
# Sinon le bot utilisera votre bureau favori automatiquement
ONEFLEX_DESK_ID=
ONEFLEX_SPACE_ID=
ONEFLEX_DESK_NAME=

# Configuration de réservation
ONEFLEX_SITE_ID=
ONEFLEX_FLOOR_ID=
ONEFLEX_ZONE_ID=

# Horaires de réservation (format HH:MM)
RESERVATION_TIME=09:00
RESERVATION_DAYS_AHEAD=7

# Jours de la semaine pour réservation récurrente (optionnel)
# 1=Lundi, 2=Mardi, 3=Mercredi, 4=Jeudi, 5=Vendredi, 6=Samedi, 7=Dimanche
# Exemple: RESERVATION_DAYS_OF_WEEK=1,3,5 pour Lundi, Mercredi, Vendredi
RESERVATION_DAYS_OF_WEEK=
